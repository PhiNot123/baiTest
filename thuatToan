public class Main {

    public static void printUnion(int[] A, int[] B, int n, int m) {
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                if (A[i] == B[j])
                    System.out.print(A[i] + " ");
            }
        }
    }
    public static void main(String[] args) {
        int[] A = {1, 3, 4, 2, 7, 10, 6, };
        int[] B = {1, 3, 9, 11, 5, 7, 13};
        printUnion(A, B, A.length, B.length);
    }
}
